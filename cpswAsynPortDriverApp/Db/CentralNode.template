record(ao, "$(DEVICE):$(PORT):CounterReset")
{
    field(PINI, "YES")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) CounterReset")
    field(SCAN, "Passive")
    info(asyn:READBACK, "1")
}
